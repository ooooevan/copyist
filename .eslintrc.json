{
  "extends": ["plugin:@copyist/core-ts"],
  "parserOptions": {
    "project": "./tsconfig.json"
  },
  "plugins": ["@copyist"],
  "rules": {
    "global-require": "off",
    "@typescript-eslint/explicit-module-boundary-types": "off",
    "no-console": "off",
    "no-unused-expressions": "off",
    "no-unsafe-assignment": "off"
  }
}
